{
  "name": "cloes-other-tabs",
  "description": "現在開いているタブ以外のタブをすべて消します",
  "version": "1.0.0",
  "author": {
    "name": "yushimatenjin"
  },
  "bugs": {
    "url": "https://github.com/yushimatenjin/close-other-tabs/issues"
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "parcel-bundler": "^1.12.4"
  },
  "homepage": "https://github.com/yushimatenjin/close-other-tabs",
  "keywords": [
    "Chrome Extensions",
    "React"
  ],
  "license": "MIT",
  "main": "dist/extension",
  "repository": {
    "url": "git@github.com:yushimatenjin/close-other-tabs.git"
  },
  "scripts": {
    "build": "yarn clean & parcel build ./src/popup.html -d extension/dist --no-source-maps  --public-url ./",
    "clean": "rm -rf ./extension/dist ./dist ./.cache",
    "lint": "eslint ./src",
    "watch": "parcel watch --no-hmr ./src/popup.html -d extension/dist --no-source-maps  --public-url ./"
  }
}
